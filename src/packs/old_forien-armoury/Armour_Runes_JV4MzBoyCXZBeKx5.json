        "scriptData": [
          {
            "label": "Rune of Resistance (Permanent)",
            "trigger": "takeDamage",
            "script": "if (args.applyAP && args.applyTB) return;\n\nconst modifier = Number(-(args.totalWoundLoss * 5));\nconst options = {fields: {modifier: modifier}, data: {other: ['hi!']}};\nlet test = await this.actor.setupCharacteristic(\"t\", options)\n\nawait test.roll();\n\nif (test.data.result.outcome === \"success\")\n  args.totalWoundLoss = 0;\n\nlet name = game.i18n.localize('Forien.Armoury.Runes.RuneOfResistance.name');\nlet effect = game.i18n.localize('Forien.Armoury.Runes.RuneOfResistance.effect');\n\ntest.data.result.other.push(`<strong>${name}:</strong> ${effect}`);\nawait test.renderRollCard();",
