<div class="forien-armoury select-item">
    {{#each styleTalents}}
        <input type="radio" name="talent" id="{{this._id}}" data-id="{{this._id}}">
        <label for="{{this._id}}">
            <img class="image" src="{{this.img}}" alt="{{this.name}}">
            <span class="name">
                {{this.name}}
            </span>
        </label>
    {{/each}}
</div>